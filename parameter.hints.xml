<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="TypeScript">
        <added pattern="setTimeout(callback,ms,args)" />
      </blacklist>
      <blacklist language="Java">
        <added pattern="org.springframework.security.config.annotation.web.AbstractRequestMatcherRegistry.antMatchers(method,antPatterns)" />
      </blacklist>
      <blacklist language="Angular HTML Template" />
      <blacklist language="JavaScript" />
      <blacklist language="Groovy" />
      <blacklist language="Kotlin" />
      <blacklist language="SQL" />
    </blacklists>
    <option id="SHOW_PARAMETER_HINT" value="false" />
    <option id="SHOW_LAMBDA_IMPLICIT_PARAMETER_RECEIVER" value="false" />
    <option id="SHOW_LAMBDA_RETURN_EXPRESSION" value="false" />
    <disabledLanguages>
      <language id="TypeScript" />
    </disabledLanguages>
  </component>
</application>